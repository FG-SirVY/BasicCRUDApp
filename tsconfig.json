{
    "compilerOptions": {
        "module": "es2022",
        "esModuleInterop": true,
        "target": "es2017",
        "noImplicitAny": true,
        "moduleResolution": "node",
        "sourceMap": true,
        "outDir": "dist",
        "baseUrl": ".",
        "jsx": "react",
        "incremental": true,
        "paths": {
            "*": [
                "node_modules/*"
            ]
        }
    },
    "include": [
        "src/server/**/*"
    ],
    "exclude": [
        "src/server/server.dev.ts"
    ]
}